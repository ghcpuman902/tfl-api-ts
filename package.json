{
  "name": "tfl-api-ts",
  "version": "1.0.0",
  "description": "TypeScript SDK for Transport for London (Tfl) API with auto-generated types and real-time data support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run generate-types && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "playground": "ts-node playground/index.ts",
    "demo": "ts-node playground/demo.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate-types": "swagger-typescript-api -p https://api.tfl.gov.uk/swagger/docs/v1 -o ./src -n tfl.ts --union-enums --extract-request-params --patch --extract-enums && ts-node src/generateEnums.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "tfl",
    "transport-for-london",
    "typescript",
    "api",
    "sdk",
    "london",
    "transport",
    "underground",
    "tube",
    "bus"
  ],
  "author": "Mangle Kuo",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manglekuo/tfl-api-ts.git"
  },
  "bugs": {
    "url": "https://github.com/manglekuo/tfl-api-ts/issues"
  },
  "homepage": "https://github.com/manglekuo/tfl-api-ts#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.0.0",
    "prettier": "^3.2.0",
    "swagger-typescript-api": "^13.0.23",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.18.2"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
